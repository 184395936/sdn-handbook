{
    "title": "SDN Handbook",
    "author": "Pengfei Ni",
    "gitbook": ">= 3.2.2",
    "language": "zh-cn",
    "plugins": [
        "mermaid",
        "github",
        "editlink",
        "github-buttons",
        "tbfed-pagefooter",
        "page-toc",
        "-lunr",
        "-search",
        "search-plus"
    ],
    "pluginsConfig": {
        "page-toc": {
          "selector": ".markdown-section h1, .markdown-section h2, .markdown-section h3",
          "position": "before-first",
          "showByDefault": true
        },
        "github-buttons": {
            "repo": "feiskyer/sdn-handbook",
            "types": [
                "star",
                "watch"
            ],
            "size": "small"
        },
        "github": {
            "url": "https://github.com/feiskyer/sdn-handbook"
        },
        "editlink": {
            "base": "https://github.com/feiskyer/sdn-handbook/blob/master/",
            "label": "编辑本页"
        },
        "tbfed-pagefooter": {
          "copyright":"&copy Pengfei Ni",
          "modify_label": "Updated at",
          "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    },
    "generator": "site"
}
